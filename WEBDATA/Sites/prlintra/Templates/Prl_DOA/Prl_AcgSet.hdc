[Permission]
DEFAULT1=ComGid
DEFAULTValue1=

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出当前页面的信息
SqlForCondition1=select Name ModeleName,Atype ModeleAtype from rt where lower(id) = lower(##Url##)

;显示公司信息
LoopSQL1=select Gid ItemGid,Name ItemName from PRL_COMPANY s where EntGid = __ENTGID__ order by code
LoopHDT1=Templates\Prl_DOA\LoopSql_Option.hdt
LoopVAR1=ComList

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
SQL1=

[AddtionalSQLUseWhere]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条，同时会使用上面的[where]中的sql语句
;示例
Sql1=

[Master]
;显示，多条记录的查询，可在HDT页面中直接使用<HDLoop>显示，并能够提供翻页。and '^^NotFirst^^' = '1'
SQL=select t.*, s.* from (select c.entgid, c.Gid ComGid, c.code ComCode, c.Name ComName, a.Gid AcgGid, a.code AcgCode, a.Name AcgName from prl_company c, prl_acg a where c.entgid = __ENTGID__ and c.entgid = a.entgid and c.gid = ##ComGid## and a.type <> 10) t, PRL_AcgSet s where t.entgid = getentgid and t.entgid = s.entgid(+) and t.comgid = s.comgid(+) and t.acggid = s.acggid(+)

Count=select count(*) from (select c.entgid, c.Gid ComGid, c.code ComCode, c.Name ComName, a.Gid AcgGid, a.code AcgCode, a.Name AcgName from prl_company c, prl_acg a where c.entgid = __ENTGID__ and c.entgid = a.entgid and c.gid = ##ComGid##) t, PRL_AcgSet s where t.entgid = getentgid and t.entgid = s.entgid(+) and t.comgid = s.comgid(+) and t.acggid = s.acggid(+)

[DefaultWhere]
;多条记录的查询的默认条件
DefaultWhere=

[Where]
;多条记录的查询的条件，可以根据页面传入的变量组合成

[GroupBy]
;多条记录的查询的分组
GroupBy=

[DefaultOrderBy]
;多条记录的查询的默认排序
DefaultOrderBy=t.AcgCode

[OrderBy]
;多条记录的查询的排序,支持多重排序排序的先后顺序由序号决定
1=##selOrderBy##

[Title]
;页面的标题，从Text1～Text30共支持30条

Text1=^^ModeleAtype^^-^^ModeleName^^

;是否翻页：1是，0否
ChangePage=0

;翻页时的每页记录数，如果没有的话，则取HDNet默认值20
ChangePageRow=10

;写入数据库日志
