[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow wf where wf.EntGid = __ENTGID__ and wf.FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName from wf_Model wm where wm.EntGid = __ENTGID__ and wm.ModelGid = ##ModelGid##

;显示本流程主表信息
Sql1=select f.*,wf.stat Flowstat,'个人报销' MCode from wf_^^ModelCode^^ f,wf_flow wf where f.EntGid = __ENTGID__ and f.EntGid = wf.EntGid and f.FlowGid = wf.FlowGid and f.FlowGid = ##FlowGid##

SQL2=select count(*) IsCEO from wf_task t where t.EntGid = __ENTGID__ and t.FlowGid = ##FlowGid## and t.Stat = 1 and lower(t.code) like '%_tc1%'

SQL3=select t.Note CEOName from wf_task t where t.EntGid = __ENTGID__ and t.FlowGid = ##FlowGid## and t.Stat = 1 and lower(t.code) like '%_tc1%'

;取出邮件
LoopSQL4=select * from WF_^^ModelCode^^_Dtl where EntGid = __ENTGID__ and FlowGid = ##FlowGid## order by AcgCode
LoopHDT4=Templates\WorkFlow\Form\^^ModelCode^^\LoopSql_EmailDtl.hdt
LoopVAR4=EmailDtl

;显示审批人
LoopSQL18=select hr.Email from hr_emp hr where hr.entgid = __ENTGID__ and hr.email is not null and exists (select 1 from wf_task t where t.EntGid = hr.EntGid and t.FlowGid = ##FlowGid## and t.ExecGid = hr.UsrGid and t.Stat = 1)
LoopHDT18=Templates\WorkFlow\Form\PrlYL_Email\Loopsql_App.hdt
LoopVAR18=AppList

;显示邮件
LoopSQL19=select 1 from dual
LoopHDT19=Templates\WorkFlow\Form\^^ModelCode^^\Loopsql_Email.hdt
LoopVAR19=EmailList

LoopSQL20=select 1 from dual
LoopHDT20=Templates\WorkFlow\Form\PrlYL_Email\Loopsql_WebEmail.hdt
LoopVAR20=EmailPhone
