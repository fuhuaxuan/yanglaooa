[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from dual where ##IsT1## = '0' union select 1 from WF_Flow wf where wf.EntGid = __ENTGID__ and wf.FlowGid = ##FlowGid## and ##IsT1## <> '0'
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，查询得到的结果可以用于HDT显示，从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;定义流程备注
SQLForCondition2=select '标题：' || ##CGTitle## TheMemo from dual

SQLForCondition3=select '单号：'|| ##Num##|| '；' || ##TheMemo##  TheFlowMemo,'待修改 - ' || ##TheMemo## ModFlowMemo,'待审批 - 发起人：' || ##CurUsrName## ||'；' || ##TheMemo## AppFlowMemo from dual

;取出下一步审批人
SqlForCondition6=select v.PostGid T2Gid from v_Post v where v.EntGid = __ENTGID__ and v.deptGid = ##FillDeptGid## and v.atype=10

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，由Insert, Update, Delete命令。从SQL1～SQL20共支持20条

;TMod-修改本流程流程备注
SQL1=update wf_flow set flownote = ##TheFlowMemo## where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and ##Stat## <> '终止' and ##IsT1## <> '0'

;T1-插入本流程主表
SQL2=insert into Wf_^^ModelCode^^ (EntGid,ModelGid,FlowGid,Num,LastUpdDate,Stat,FillUsrGid,FillUsrCode,FillUsrName,FillDeptGid,FillDeptCode,FillDeptName)select __ENTGID__,##ModelGid##,##FlowGid##,##Num##,sysdate,##Stat##,__USRGID__,__USRCODE__,__USRNAME__,##FillDeptGid##,##FillDeptCode##,##FillDeptName## from dual where ##IsT1## = '0'

;TMod-更新本流程主表信息，保存，提交
SQL3=update Wf_^^ModelCode^^ set (LastUpdDate,Stat,CGTitle,CGMemo) = (select sysdate,##Stat##,##CGTitle##,##CGMemo## from dual) where EntGid = __ENTGID__  and FlowGid = ##FlowGid## and ##Stat## <> '终止'

;更新本流程流程主表信息，终止
SQL4=update Wf_^^ModelCode^^ set (LastUpdDate,Stat,EndMemo) = (select sysdate,##Stat##,##EndMemo## from dual) where EntGid = __ENTGID__  and FlowGid = ##FlowGid## and ##Stat## = '终止'

;删除本流程中未审批的审批人
SQL5=delete from Wf_^^ModelCode^^_App where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and AppDate is null

;插入本流程发起人信息
SQL6=insert into Wf_^^ModelCode^^_App (EntGid,ModelGid,FlowGid,Gid,AppGid,AppCode,AppName,AppType,AppAssign,AppMemo,AppDate) select __ENTGID__,##ModelGid##,##FlowGid##,sys_guid(),__USRGID__,__USRCODE__,__USRNAME__,10,##Stat##,'',sysdate from dual where ##Stat## <> '保存'

;删除本流程全部附件信息
SQL7=delete from Wf_^^ModelCode^^_Attach where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and ##Stat## <> '终止'

;插入本流程明细信息
SQL8=delete from Wf_^^ModelCode^^_Dtl where EntGid = __ENTGID__ and FlowGid = ##FlowGid## and ##Stat## <> '终止'

;--如果终止或作废后，存在对业务数据的回写，使用下列语句，并创建对应过程
;--过程命名："Hdnet_doWF_模型代码"
;--参数1：EntGid
;--参数2：FlowGid
;--参数3：状态（默认传入"终止"有效，可传入"作废"等，具体可在过种中配置）
;--管理员作废时，也会调用此过程，默认传入"终止"
;SQL10=call Hdnet_doWF_^^ModelCode^^(__ENTGID__,##FlowGid##,##Stat##)

;--更新审批人
SQL11=call P_^^ModelCode^^_doApp(__ENTGID__,##ModelGid##,##FlowGid##,##Stat##)

;插入本流程明细信息
LoopSql1=insert into Wf_^^ModelCode^^_Dtl (EntGid,ModelGid,FlowGid,Gid,Line,CGMemo) select __ENTGID__,##ModelGid##,##FlowGid##,sys_guid(),##%Line%##,##%CGMemo%## from dual where ##Stat## <> '终止'

;插入本流程附件信息
AttachSqlAdd1=insert into Wf_^^ModelCode^^_Attach (EntGid,ModelGid,FlowGid,Gid,AttachTitle,AttachUrl,AttachSize) select __ENTGID__,##ModelGid## ,##FlowGid##,@@AttachID@@, @@FileName@@, @@FilePath@@,@@FileSize@@ from dual where ##Stat## <> '终止'

