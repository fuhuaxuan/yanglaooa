<!--显示审批人信息-->
<HDIF EqualNot(<HDPrint Value=#IsShow# Replace=#None#>, 0)>
<HDPrint Value=#AppList# Replace=#None#>
</HDIF>

<h3 class="SubTitleBar" >
<TABLE cellspacing="1" cellpadding="0" width="100%">
  <col style="padding-left:4px;">
  <col style="width:20px;" align="center">
  <TR>
    <TD align="left" style="padding-left:10px;"><b>单据信息</b></TD>
    <TD onclick="JQExpand('tb_Order');" style="cursor:hand" class="IconUp">　&nbsp;</TD>
  </TR>
</TABLE>
</h3>

<table cellpadding="0" cellspacing="1" class="ListBar" width="100%" id="Order">
  <col style="padding-left:4px;width:10%">
  <col style="padding-left:4px;width:23%">
  <col style="padding-left:4px;width:10%">
  <col style="padding-left:4px;width:23%">
  <col style="padding-left:4px;width:10%">
  <col style="padding-left:4px;width:23%">
  <tr valign="top">
    <td class="DtlBarTitle">部门类型:<font color="red">*</font></td>
    <td class="listtd">
      <input type="radio" name="DeptType" value="总部管理部门" class="JQRadio JQMust  JQSave" checked>总部管理部门
      <input type="radio" name="DeptType" value="项目部门" class="JQRadio JQMust  JQSave">项目部门
      <script language="javascript">
        $("input[name=DeptType][value='<HDPrint Value=#DeptType# Replace=#DBField2Js#>']").prop("checked",true);
      </script>
    </td>
    <td class="DtlBarTitle">出差时间:<font color="red">*</font></td>
    <td class="listtd">
      <INPUT TYPE="text" NAME="CCBDate" maxlength="10" value="<HDPrint Value=#CCBDate# Replace=#DBField2Input#>" class="JQDate" onblur="callDate(this)" style="width:100px">~
      <INPUT TYPE="text" NAME="CCEDate" maxlength="10" value="<HDPrint Value=#CCEDate# Replace=#DBField2Input#>" class="JQDate" onblur="callDate(this)" style="width:100px">
    </td>
    <td class="DtlBarTitle">出差地点:<font color="red">*</font></td>
    <td class="listtd">
      <INPUT TYPE="text" NAME="CCPlace" maxlength="32" value="<HDPrint Value=#CCPlace# Replace=#DBField2Input#>" class="JQText JQMust">
    </td>
  </tr>
  <tr valign="top">
    <td class="DtlBarTitle">出差原因:<font color="red">*</font><br><font color="gray">(1000字以内)</font></td>
    <td class="listtd" colspan="5"><textarea rows="5" name="Memo" class="JQText JQMust" maxlength="1000"><HDPrint Value=#Memo# Replace=#DBField2InputField#></textarea></td>
  </tr>
  <tr JQ="TMod_1" style="display:none">
    <td class="DtlBarTitle">终止原因:<font color="red">*</font></td>
    <td class="listtd" colspan="5"><textarea rows="5" id ="EndMemo" name="EndMemo" class="JQText JQEnd" maxlength="1000"><HDPrint Value=#EndMemo# Replace=#DBField2InputField#></textarea></td>
