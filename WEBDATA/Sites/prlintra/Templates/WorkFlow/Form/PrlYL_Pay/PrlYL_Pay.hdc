[Ajax]
;此Section“[Ajax]”,表明以下内容是为了Ajax应用而配置的文件。
;需要显示一条的内容，可以支持21条, Detail, Detail1～Detail20
Detail=select nvl(v.LeftFee - v.YesFee,0) LeftFee from v_PRL_ACG_Fee v where v.EntGid = __ENTGID__ and v.ACGGID = ##ACGGID## and v.ComGid = ##ComGid## and v.Year = to_char(sysdate,'YYYY') and ##OpAtype## = '1'

;需要循环显示的内容,可以支持11条，Master, Master1～Master10
Master=select b.CODE DOACODE,nvl(b.Memo,b.Name) DOAName from PRL_acg_DOA a, prl_doa b where a.acggid = ##AcgGid##  AND B.GID = A.DOAGID and a.Fatype = '付款单' and ##OpAtype## = '3' order by b.code

