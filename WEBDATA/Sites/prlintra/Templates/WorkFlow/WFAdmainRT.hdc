[Master]
;显示，多条记录的查询，可在HDT页面中直接使用<HDLoop>显示，并能够提供翻页。
SQL=select a.*,a.UsrGidEx, substr(a.RTID, 1, 1) RtID1, substr(a.RTID, 2, 1) RtID2,substr(a.RTID, 3, 1) RtID3,substr(a.RTID, 4, 1) RtID4, b.name usrname, b.code usrcode, b.gid UsrGid from wf_rt a, v_usr b where A.EntGid = ##EntGid## and a.modelgid = ##modelgid## and b.EntGid = ##EntGid## and b.gid=a.usrgidex order by b.Code

Count=select count(*) from wf_rt a, v_usr b where A.EntGid = ##EntGid## and a.modelgid = ##modelgid## and b.EntGid = ##EntGid## and b.gid=a.usrgidex

